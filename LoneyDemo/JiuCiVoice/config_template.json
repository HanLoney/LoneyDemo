{
  "authentication": {
    "app_id": "YOUR_APP_ID",
    "access_token": "YOUR_ACCESS_TOKEN"
  },
  "service": {
    "endpoint": "wss://openspeech.bytedance.com/api/v3/tts/unidirectional/stream",
    "max_message_size": 10485760
  },
  "tts": {
    "speaker": "S_HLw7rGSx1",
    "audio_params": {
      "format": "wav",
      "sample_rate": 24000,
      "enable_timestamp": true
    },
    "additions": {
      "disable_markdown_filter": false
    }
  },
  "jiuci_voice": {
    "default_output_dir": "./output",
    "auto_save": true,
    "filename_template": "jiuci_reply_{timestamp}.wav",
    "voice_profiles": {
      "sweet": "S_female_01",
      "gentle": "S_female_02", 
      "lively": "S_female_03",
      "mature": "S_female_04"
    }
  },
  "logging": {
    "level": "INFO"
  }
}